# Vercel Postgres接続URL
POSTGRES_URL=

# Google AI Studio APIキー
GEMINI_API_KEY=

# NextAuth設定
NEXTAUTH_URL=https://your-domain.vercel.app
NEXTAUTH_SECRET=  # 32バイト以上のランダム文字列（openssl rand -base64 32）

# 暗号化キー（機密データ用）
ENCRYPTION_KEY=  # 64文字のHEX文字列（node -e "console.log(require('crypto').randomBytes(32).toString('hex'))"）

# 初期システム管理者（デプロイ後に/api/initを呼ぶと作成される）
INITIAL_ADMIN_EMAIL=admin@example.com
INITIAL_ADMIN_PASSWORD=  # 8文字以上、英字と数字を含む
